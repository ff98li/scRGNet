citHeader("To cite scRGNet in publications use:")

citEntry(
  entry    = "unpublished",
  title    = "scRGNet: Generating cell networks from encoded scRNA-seq data",
  author   = "Feifei Li",
  year     = "2021",
  url      = "https://github.com/ff98li/scRGNet.git",
  textVersion = paste(
        "Li, F. (2021) scRGNet: Generating cell networks from encoded scRNA-seq data. Unpublished. URL: https://github.com/ff98li/scRGNet.git"
  ),
  note     = "On-going development. Initial stage of the package for BCB410 course project submission."
)
